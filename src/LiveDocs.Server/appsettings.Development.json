{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LiveDocs": {
    "ApplicationName": "LiveDocs DEV",
    "DocumentationFolder": "../../tests/docs",
    "DefaultDocuments": [
      "index",
      "readme"
    ],
    "Documents": {
      "Priority": [ "html", "markdown" ],
      "ShowDownloadOriginal": true,
      "Drawio": {
        "Enabled": false
      },
      "DrawioSvg": {
        "ShowDocumentNameAsTitle": true,
        "DefaultBackground": "Light"
      }
    },
    "Navigation": {
      "Header": [
        {
          "Url": "https://www.github.com",
          "Text": "External Test",
          "NewTab": true
        },
        {
          "Url": "/code",
          "Text": "Some Code"
        },
        {
          "Url": "/about-livedocs",
          "Text": "About"
        },
        {
          "Url": "/test-project/document1",
          "Text": "Subproject",
          "NewTab": true,
          "SubElements": [
            {
              "Url": "/very-long-document",
              "Text": "Very long document"
            },
            {
              "Url": "/images",
              "Text": "Some images"
            }
          ]
        }
      ],
      "Footer": [
        {
          "Text": "Footer 1",
          "Url": "/nested/nested"
        },
        {
          "Text": "Footer 2"
        }
      ]
    }
  }
}